{
	"name": "svelte-konva",
	"version": "0.0.1",
	"scripts": {
		"dev": "set NODE_ENV=development&& vite dev",
		"build-app": "set NODE_ENV=production&& vite build",
		"preview": "vite preview",
		"build": "set NODE_ENV=production&& svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"plop": "plop",
		"prepack": "plop",
		"predev": "plop"
	},
	"peerDependencies": {
		"konva": ">=8",
		"canvas": ">=2"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.44",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@tailwindcss/typography": "^0.5.7",
		"@types/lodash.clonedeep": "^4.5.7",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"canvas": "^2.10.1",
		"daisyui": "^2.31.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"konva": "^8.3.13",
		"lodash.clonedeep": "^4.5.0",
		"plop": "^3.1.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-highlight": "^6.2.1",
		"svelte-local-storage-store": "^0.3.1",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module"
}
